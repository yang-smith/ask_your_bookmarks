{
  "manifest_version": 2,
  "name": "Ask Your Bookmarks",
  "version": "1.0",
  "description": "Enhance your bookmark search with vectorization technology.",
  "permissions": ["storage", "activeTab", "bookmarks", "<all_urls>"],
  "background": {
      "scripts":["background.global.js"]
  },
  "browser_action": {
      "default_popup": "index.html"
  }
}
